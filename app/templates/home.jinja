{% extends "base.jinja" %}
{% block header %}About{% endblock %}

{% block content %}
<div class = "container">
    <img src = "{{user['img']}}" class = "about-img"/>
    <div class = "about-div">
        <h1 class = "about-title">Who is {{ title }}?</h1>
        <p class = "about-txt text-center">{{user['about']}}</p>
    </div>    
</div>
   
{% endblock %}
